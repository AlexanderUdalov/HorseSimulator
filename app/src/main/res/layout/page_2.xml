<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:paddingLeft="20dp"
            android:paddingTop="8dp"
            android:paddingRight="20dp">
            <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/eat_grass"
                android:id="@+id/button_eat_grass"/>
        </TableLayout>
        <TableRow
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            >
            <TableRow
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    android:id="@+id/eat_grass_stamina_text"/>
                <ImageView
                    android:layout_width="20sp"
                    android:layout_height="20sp"
                    android:id="@+id/eat_grass_stamina_image"/>
            </TableRow>
            <TableRow
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="15dp"
                    android:paddingRight="5dp"
                    android:id="@+id/eat_grass_satiety_text"/>
                <ImageView
                    android:layout_width="20sp"
                    android:layout_height="20sp"
                    android:id="@+id/eat_grass_satiety_image"/>
            </TableRow>
            <TableRow
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="15dp"
                    android:paddingRight="5dp"
                    android:id="@+id/eat_grass_happiness_text"/>
                <ImageView
                    android:layout_width="20sp"
                    android:layout_height="20sp"
                    android:id="@+id/eat_grass_happiness_image"/>
            </TableRow>
        </TableRow>
        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:paddingTop="8dp"
            android:paddingLeft="20dp"
            android:paddingRight="20dp">
            <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/stealing_food"
                android:id="@+id/button_stealing_food"/>
        </TableLayout>
        <TableRow
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            >
            <TableRow
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content">
                <TextView
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/stealing_food_stamina_text"/>
                <ImageView
                    android:layout_width="20sp"
                    android:layout_height="20sp"
                    android:id="@+id/stealing_food_stamina_image"/>
            </TableRow>
            <TableRow
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="15dp"
                    android:paddingRight="5dp"
                    android:id="@+id/stealing_food_satiety_text"/>
                <ImageView
                    android:layout_width="20sp"
                    android:layout_height="20sp"
                    android:id="@+id/stealing_food_satiety_image"/>
            </TableRow>
            <TableRow
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="15dp"
                    android:paddingRight="5dp"
                    android:id="@+id/stealing_food_happiness_text"/>
                <ImageView
                    android:layout_width="20sp"
                    android:layout_height="20sp"
                    android:id="@+id/stealing_food_happiness_image"/>
            </TableRow>
        </TableRow>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:id="@+id/stealing_food_other_text"/>
        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:paddingTop="8dp"
            android:paddingLeft="20dp"
            android:paddingRight="20dp">
            <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/begging_sugar"
                android:id="@+id/button_begging_sugar"/>
        </TableLayout>
        <TableRow
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            >
            <TableRow
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content">
                <TextView
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/begging_sugar_stamina_text"/>
                <ImageView
                    android:layout_width="20sp"
                    android:layout_height="20sp"
                    android:id="@+id/begging_sugar_stamina_image"/>
            </TableRow>
            <TableRow
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="15dp"
                    android:paddingRight="5dp"
                    android:id="@+id/begging_sugar_satiety_text"/>
                <ImageView
                    android:layout_width="20sp"
                    android:layout_height="20sp"
                    android:id="@+id/begging_sugar_satiety_image"/>
            </TableRow>
            <TableRow
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="15dp"
                    android:paddingRight="5dp"
                    android:id="@+id/begging_sugar_happiness_text"/>
                <ImageView
                    android:layout_width="20sp"
                    android:layout_height="20sp"
                    android:id="@+id/begging_sugar_happiness_image"/>
            </TableRow>
        </TableRow>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:id="@+id/begging_sugar_other_text"/>
        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:paddingTop="8dp"
            android:paddingLeft="20dp"
            android:paddingRight="20dp">
            <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/ask_for_food"
                android:id="@+id/button_ask_for_food"/>
        </TableLayout>
        <TableRow
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            >
            <TableRow
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content">
                <TextView
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/ask_for_food_stamina_text"/>
                <ImageView
                    android:layout_width="20sp"
                    android:layout_height="20sp"
                    android:id="@+id/ask_for_food_stamina_image"/>
            </TableRow>
            <TableRow
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="15dp"
                    android:paddingRight="5dp"
                    android:id="@+id/ask_for_food_satiety_text"/>
                <ImageView
                    android:layout_width="20sp"
                    android:layout_height="20sp"
                    android:id="@+id/ask_for_food_satiety_image"/>
            </TableRow>
            <TableRow
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="15dp"
                    android:paddingRight="5dp"
                    android:id="@+id/ask_for_food_happiness_text"/>
                <ImageView
                    android:layout_width="20sp"
                    android:layout_height="20sp"
                    android:id="@+id/ask_for_food_happiness_image"/>
            </TableRow>
        </TableRow>
        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:paddingTop="8dp"
            android:paddingLeft="20dp"
            android:paddingRight="20dp">
            <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/eat_apple"
                android:id="@+id/button_eat_apple"/>
        </TableLayout>
        <TableRow
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            >
            <TableRow
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content">
                <TextView
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/eat_apple_stamina_text"/>
                <ImageView
                    android:layout_width="20sp"
                    android:layout_height="20sp"
                    android:id="@+id/eat_apple_stamina_image"/>
            </TableRow>
            <TableRow
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="15dp"
                    android:paddingRight="5dp"
                    android:id="@+id/eat_apple_satiety_text"/>
                <ImageView
                    android:layout_width="20sp"
                    android:layout_height="20sp"
                    android:id="@+id/eat_apple_satiety_image"/>
            </TableRow>
            <TableRow
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="15dp"
                    android:paddingRight="5dp"
                    android:id="@+id/eat_apple_happiness_text"/>
                <ImageView
                    android:layout_width="20sp"
                    android:layout_height="20sp"
                    android:id="@+id/eat_apple_happiness_image"/>
            </TableRow>
        <TableRow
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="15dp"
                android:paddingRight="5dp"
                android:id="@+id/eat_apple_gold_apple_text"/>
            <ImageView
                android:layout_width="20sp"
                android:layout_height="20sp"
                android:id="@+id/eat_apple_gold_apple_image"/>
        </TableRow>
        </TableRow>
        <View
            android:layout_width="match_parent"
            android:layout_height="60dp" />
    </LinearLayout>
</ScrollView>